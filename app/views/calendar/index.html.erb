<% if current_user  %>
	<%= render 'header' %>
<% end %>
<% if notice %><p class="alert"><%= notice %></p><% end %>
<div class="container">
<p>
<strong>
<% if session['calendarUser'].to_i == current_user.id %>
My calendar
<% else %> 
<%= "#{User.find(session['calendarUser']).first_name}'s Calendar" %>
<% end %>
